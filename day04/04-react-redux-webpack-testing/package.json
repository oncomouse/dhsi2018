{
  "name": "react-redux-webpack-starter",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/oncomouse/react-redux-webpack-starter.git"
  },
  "main": "app/index.jsx",
  "title": "Testing React-Redux App",
  "scripts": {
    "start": "npm-run-all --parallel webpack jsonplaceholder",
    "webpack": "webpack-dev-server",
    "jsonplaceholder": "node node_modules/jsonplaceholder/index",
    "build": "cross-env NODE_ENV=production webpack",
    "lint": "npm-run-all \"lint:* -- {@}\" --",
    "lint:js": "eslint --ext .js --ext .jsx app/",
    "lint:css": "stylelint \"app/stylesheets/**/*.scss\" --cache --cache-location \"./.stylelintcache\"",
    "cover": "cross-env BABEL_ENV=test NODE_ENV=coverage nyc -r text -r lcov npm-run-all mocha",
    "mocha:watch": "npm-run-all \"mocha -- --watch\"",
    "mocha": "mocha-webpack test.prepare.js \"app/**/*.spec.js\" \"app/**/*.spec.jsx\"",
    "test": "npm-run-all lint mocha"
  },
  "author": "Andrew Pilsch <apilsch@tamu.edu> (https://andrew.pilsch.com)",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-tailcall-optimization": "^1.0.11",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-traverse": "^6.13",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "extracted-loader": "^1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "noop-webpack-plugin": "^1.0.1",
    "normalize-scss": "^7.0.1",
    "normalizr": "^3.2.4",
    "prop-types": "^15.5.10",
    "ramda": "^0.25.0",
    "react": "^16",
    "react-dom": "^16",
    "react-redux": "^5.0.7",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "fetch-mock": "^6.4.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^11.11.0",
    "jsdom-global": "^3.0.2",
    "jsonplaceholder": "^0.3.3",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.8.0",
    "postcss-syntax": "^0.26.1",
    "redux-mock-store": "^1.5.1",
    "sinon": "^5.0.10",
    "sinon-chai": "^3.1.0",
    "stylelint": "^9.2.1",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "include": [
      "app/**/*.js",
      "app/**/*.jsx"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/*.spec.jsx",
      "build"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
